<%= form_for :post, url: posts_path do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>
 
<p>
  <%= f.label :image %><br>
  <%= f.file_field :image %>
</p>

  <p>
    <%= f.label :address %><br>
    <%= f.text_area :address %>
  </p>
  <div class="field">
    <%= f.label :latitude %><br>
    <%= f.text_field :latitude %>
  </div>
  <div class="field">
    <%= f.label :longitude %><br>
    <%= f.text_field :longitude %>
  </div>

  <p>
    <%= f.submit %>
  </p>
<% end %>