<h1>Listing articles</h1>
 
<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
    <th>address</th>
    <th>lat</th>
    <th>long</th>
    <th>dist</th>

  </tr>
 
  <% @posts.each do |post| %>
    <tr>
      <td><%= post.title %></td>
      <td><%= post.text %></td>
      <td><%= post.address %></td>
      <td><%= post.latitude %></td>
      <td><%= post.longitude %></td>

 

      <td><b> <%= post.distance.round(1) %> miles</b></td>
    </tr>
 


  <% end %>
</table>

