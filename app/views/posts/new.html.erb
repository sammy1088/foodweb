<div id="wrap">
  <div class="container">
  <center>
<%= form_for :post, url: posts_path do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title, class: "form-control" %>
  </p>
 
  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text, class: "form-control" %>
  </p>

  <p>
    <%= f.label :price %><br>
    <%= f.text_area :price, class: "form-control" %>
  </p>

  <p>
    <%= f.label :quantity %><br>
    <%= f.text_area :quantity, class: "form-control" %>
  </p>
 
 
<p>
  <%= f.label :image %><br>
  <%= f.file_field :image %>
</p>

  <p>
    <%= f.label :address %><br>
    <%= f.text_area :address, class: "form-control" %>
  </p>
<div class="btn-group">
    <%= f.submit nil, :class => 'btn btn-primary' %>
  </div>
  </p>
<% end %>
    </center>
</div>
  </div>